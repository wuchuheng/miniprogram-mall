.container{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    padding-top: 10rpx;
}
.inner-container{
    width: 730rpx;
    height: 300rpx;
    border: 1px dashed #DCEBE6;
}
.grid-item{
    width: 200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.img{
    width: 60rpx;
    height: 60rpx;
}
.text{
    margin-top: 6rpx;
    font-size: 26rpx;
    color: #333333
}
